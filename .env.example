# Application Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Proxy Configuration
PROXY_TIMEOUT=30000
PROXY_MAX_REDIRECTS=5
PROXY_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"

# Session Configuration
SESSION_TTL=3600
SESSION_MAX_LENGTH=40

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Security Configuration
CORS_ORIGIN=http://localhost:3333,http://localhost:3000,http://127.0.0.1:3333,http://127.0.0.1:3000*
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100
LOG_TO_FILE=false
LOG_DIR=logs
